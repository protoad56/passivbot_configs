{
    "multicore": false,
    "orderbook_used": false,
    "hedge_mode": true,
    "persisted_tick_purge_retain_count": 3000,
    "initial_entry_search_attempts": 5,
    "state_synchronize_interval_s": 5,
    "initial_entry_ignore_not_found": true,
    "override_max_exposure_ratio_check": true,
    "plugins": {
        "MultiAutoreducePlugin": {
            "reduce_interval": "43200s",
            "profit_percentage_used_for_reduction": 0.1,
            "activate_above_upnl_pct": 350,
            "max_age_income": "1D"
        }
    },
    "dynamic_entry": {
        "max_positions": 1,
        "enabled": true,
        "dynamic_entry_sweep_frequency": "5s",
        "filters": [
            {
                "filter": "SymbolFilter",
                "config": {
                    "whitelist": [
                        ".*USDT"
                    ],
                    "blacklist": [
                        "LISTAUSDT",
                        "TURBOUSDT",
                        "AAVEUSDT",
                        "AGIXUSDT",
                        "OCEANUSDT",
                        "BTCUSDT",
                        "ETHUSDT",
                        "SOLUSDT",
                        "BTCDOMUSDT",
                        "EURUSDT",
                        "GBPUSDT",
                        "JPYUSDT",
                        "USDUSDT",
                        "BUSDUSDT",
                        "USDCUSDT",
                        ".*DOWN/.*",
                        ".*UP/.*"
                    ]
                }
            },
            // {
            //     "filter": "MinNotionalFilter",
            //     "config": {
            //         "less_than": 10
            //     }
            // },
            {
                "filter": "AgeFilter",
                "config": {
                    "older_than": "30D"
                }
            },
            // {
            //     "filter": "Last24hVolumeFilter",
            //     "config": {
            //       "sort": "desc",
            //       "top": 30
            //     }
            // },
            {
                "filter": "Last24hPriceChangePctFilter",
                "config": {
                    "sort": "desc",
                    "sort_absolute": true,
                    "top": 30
                }
            }
        ],
        "symbol_config_template": {
            "exchange_leverage": 50,
            "long": {
                "enabled": true,
                "strategy": "BigLongStrategy",
                "wallet_exposure_ratio": 2,
                "tick_execution_interval_ms": 250,
                "strategy_config": {
                    "entry_order_type": "LIMIT",
                    "limit_orders_reissue_threshold": 0.00001,
                    "repost_lower_allowed": false,
                    "dca": {
                        "allow_add_new_smaller_dca": false,
                        "period": "3D",
                        "period_timeframe": "1m",
                        "algo": "LINEAR",
                        "nr_clusters": 4,
                        "outer_price_distance": 0.04,
                        "ratio_power": 1.6
                    },
                    "tp": {
                        "minimum_tp": 0.002
                    }
                }
            },
            "short": {
                "enabled": true,
                "strategy": "BigShortStrategy",
                "wallet_exposure_ratio": 2,
                "tick_execution_interval_ms": 250,
                "strategy_config": {
                    "entry_order_type": "LIMIT",
                    "limit_orders_reissue_threshold": 0.00001,
                    "repost_lower_allowed": false,
                    "dca": {
                        "allow_add_new_smaller_dca": false,
                        "period": "3D",
                        "period_timeframe": "1m",
                        "algo": "LINEAR",
                        "nr_clusters": 4,
                        "outer_price_distance": 0.04,
                        "ratio_power": 1.6
                    },
                    "tp": {
                        "minimum_tp": 0.002
                    }
                }
            }
        }
    }
}